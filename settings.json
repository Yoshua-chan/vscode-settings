{
    "workbench.colorTheme": "Gruvbox Dark Soft",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediEnabled": false,
    "editor.mouseWheelZoom": true,
    "editor.wordWrap": "on",
    "terminal.integrated.shell.linux": "/bin/zsh",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "editor.snippetSuggestions": "top",
    "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "terminal.integrated.fontFamily": "'Noto Mono for Powerline",
    //"terminal.integrated.fontFamily": "'Fira Code",
    "C_Cpp.updateChannel": "Insiders",
    "liveServer.settings.donotVerifyTags": true,
    "workbench.iconTheme": "gruvbox-material-icon-theme",
    "eslint.alwaysShowStatus": true,
    "python.languageServer": "Pylance",
    "editor.fontFamily": "'Fira Code', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": "'ss03', 'ss04', 'ss07', 'zero'",
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.leader": " ",
    "vim.useSystemClipboard": true,
    "keyboard.dispatch": "keyCode",
    "editor.rulers": [
        120
    ],
    "vim.hlsearch": true,
    "vim.smartRelativeLine": true,
    "bracket-pair-colorizer-2.colors": [
        "Default",
        "#ffbb46",
        //"#649c6e",
        "LightGreen",
        "#b66385",
        "#80a599",
        "DarkOrange",
        "#649c6e",
    ],
    "peacock.darkForegroundColor": "",
    "bracket-pair-colorizer-2.colorMode": "Independent",
    "editor.minimap.enabled": false,
    "diffEditor.codeLens": true,
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "cmake.configureOnOpen": false,
    "material-icon-theme.activeIconPack": "angular_ngrx",
    "errorLens.enabled": false,
    
}
